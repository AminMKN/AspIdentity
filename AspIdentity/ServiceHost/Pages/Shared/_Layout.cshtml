<!DOCTYPE html>
<html lang="fa">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] | کاربران</title>
    <script src="~/sweetalert2/dist/sweetalert2.min.js"></script>
    <link href="~/sweetalert2/dist/sweetalert2.min.css" rel="stylesheet">
    <link rel="shortcut icon" href="~/AdminTheme/assets/img/favicon.ico">
    <link href="~/AdminTheme/assets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="~/AdminTheme/assets/css/sb-admin-2.css" rel="stylesheet">
    @await RenderSectionAsync("Css", required: false)
</head>

<body class="bg-gradient-primary" dir="rtl">
    <div class="container">
        @RenderBody()
    </div>
    <script src="~/AdminTheme/assets/vendor/jquery/jquery.min.js"></script>
    <script src="~/AdminTheme/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminTheme/assets/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/AdminTheme/assets/js/sb-admin-2.min.js"></script>
    <script src="~/sweetalert2/dist/sweetalert2.all.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>